{
    "name": "roots/bedrock",
    "type": "project",
    "license": "MIT",
    "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
    "homepage": "https://roots.io/bedrock/",
    "authors": [
        {
            "name": "Scott Walkinshaw",
            "email": "scott.walkinshaw@gmail.com",
            "homepage": "https://github.com/swalkinshaw"
        },
        {
            "name": "Ben Word",
            "email": "ben@benword.com",
            "homepage": "https://github.com/retlehs"
        }
    ],
    "keywords": [
        "bedrock",
        "composer",
        "roots",
        "wordpress",
        "wp",
        "wp-config"
    ],
    "support": {
        "issues": "https://github.com/roots/bedrock/issues",
        "forum": "https://discourse.roots.io/category/bedrock"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        },
        "1": {
            "type": "package",
            "package": {
                "name": "advanced-custom-fields/advanced-custom-fields-pro",
                "version": "5.9.3",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%ACF_PRO_KEY}&t={%version}"
                },
                "require": {
                    "composer/installers": "^1.4",
                    "ffraenz/private-composer-installer": "^5.0"
                }
            }
        },
        "2": {
            "type": "composer",
            "url": "https://composer.deliciousbrains.com/D4A7ACB70D1F4E28E3BF5453A3FC8564"
        },
        "3": {
            "type": "vcs",
            "url": "git@github.com:13milliseconds/wp-fastest-cache-premium.git"
        },
        "4": {
            "type": "vcs",
            "url": "git@github.com:13milliseconds/simple-podcast-press.git"
        },
        "5": {
            "type": "vcs",
            "url": "git@github.com:13milliseconds/tse-shortcodes.git"
        },
        "my-yoast": {
            "type": "composer",
            "url": "https://my.yoast.com/packages/"
        }
    },
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.8",
        "vlucas/phpdotenv": "^4.1.8",
        "oscarotero/env": "^2.1",
        "roots/bedrock-autoloader": "^1.0",
        "roots/wordpress": "5.5.3",
        "roots/wp-config": "1.0.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "13milliseconds/wp-fastest-cache-premium": "dev-master",
        "13milliseconds/simple-podcast-press": "dev-master",
        "13milliseconds/tse-shortcodes": "dev-master",
        "deliciousbrains-plugin/wp-migrate-db-pro": "^1.8.4",
        "deliciousbrains-plugin/wp-migrate-db-pro-media-files": "*",
        "deliciousbrains-plugin/wp-offload-media": "2.3.2",
        "advanced-custom-fields/advanced-custom-fields-pro": "^5.9.3",
        "wpackagist-plugin/accelerated-mobile-pages": "^1.0.50",
        "wpackagist-plugin/amazon-s3-and-cloudfront": "^2.5",
        "wpackagist-plugin/user-photo": "^0.9.10",
        "wpackagist-plugin/akismet": "^4.1.1",
        "wpackagist-plugin/classic-editor": "^1.5",
        "wpackagist-plugin/custom-banners": "^3.2",
        "wpackagist-plugin/disqus-comment-system": "^3.0.17",
        "wpackagist-plugin/duplicate-post":"3.2.6",
        "wpackagist-plugin/duracelltomi-google-tag-manager": "^1.9.2",
        "wpackagist-plugin/easy-smooth-scroll-links": "^2.1.1",
        "wpackagist-plugin/editorial-calendar": "^3.7.2",
        "wpackagist-plugin/ewww-image-optimizer": "^5.3.2",
        "wpackagist-plugin/jetpack": "^9.1",
        "wpackagist-plugin/mailchimp": "^1.5.7",
        "wpackagist-plugin/official-facebook-pixel": "^2.0.2",
        "wpackagist-plugin/recent-posts-widget-extended": "^0.9.9.7",
        "wpackagist-plugin/relevanssi": "^4.1.4",
        "wpackagist-plugin/schema-and-structured-data-for-wp": "^1.9.54",
        "wpackagist-plugin/shiftnav-responsive-mobile-menu": "^1.6.3",
        "wpackagist-plugin/simple-tags": "^2.5.4",
        "wpackagist-plugin/social-warfare": "^4.0.1",
        "wpackagist-plugin/super-progressive-web-apps": "^2.0.2",
        "wpackagist-plugin/subheading": "^1.8.1",
        "wpackagist-plugin/svg-support": "^2.3.15",
        "wpackagist-plugin/testimonials-widget": "^4.0.1",
        "wpackagist-plugin/upprev": "^3.4.1",
        "wpackagist-plugin/wordfence": "^7.3.2",
        "wpackagist-plugin/wp-disable": "^1.5.21",
        "wpackagist-plugin/wp-fastest-cache": "^0.9.1.0",
        "wpackagist-plugin/wp-maximum-execution-time-exceeded": "^1.0.2",
        "wpackagist-plugin/wp-to-twitter": "^3.3.12",
        "wpackagist-plugin/otf-regenerate-thumbnails": "0.3",
        "yoast/yoast-news-seo": "^12.6",
        "yoast/wordpress-seo-premium": "^15.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5.6",
        "roave/security-advisories": "dev-master"
      },
      "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
      },
      "minimum-stability": "dev",
      "prefer-stable": true,
      "extra": {
        "installer-paths": {
          "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
          "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
          "web/app/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "web/wp"
      },
      "scripts": {
        "post-root-package-install": [
          "php -r \"copy('.env.example', '.env');\""
        ],
        "test": [
          "phpcs"
        ]
      }
    }
